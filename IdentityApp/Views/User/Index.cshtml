@model IEnumerable<IdentityUser>

    <a href="" class="btn btn-success" asp-action="Create" >Kullanıcı Ekle</a>
    <hr>
    <table class="table table-sm table-bordered">
<thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
    </tr>
</thead>
<tbody>
    @foreach(var user in Model){
        <tr>
            <td>@user.Id</td>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>@user.PhoneNumber</td>
            <td>
                <a href="" class="btn btn-primary btn-sm">Edit</a>
                <a href="" class="btn btn-danger btn-sm">Güncele</a>
            </td>
        </tr>
    }
</tbody>
</table> 